  "contributors": [
    "Thomas Tran <tldinteractive@gmail.com>",
    "Yaaccount <tnuoccaay@gmail.com>"
  ],
  "bugs": {
    "url": "https://github.com/proximax-storage/js-xpx-chain-library/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/proximax-storage/js-xpx-chain-library.git"
  },
  "publishConfig": {
    "registry": "https://nexus.internal.proximax.io/repository/npm-private/"
  },
  "files": [
    "/dist"
  ],
  "dependencies": {
    "bufferutil": "^3.0.5",
    "crypto-js": "^3.1.9-1",
    "flatbuffers": "1.9.0",
    "js-sha3": "^0.6.1",
    "lodash": "^4.17.10",
    "ripemd160": "^2.0.1",
    "superagent": "^3.8.3",
    "utf-8-validate": "^4.0.2",
    "ws": "^5.2.0",
    "crypto-browserify": "3.12.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "6.24.1",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "expect.js": "~0.3.1",
    "mocha": "^6.1.4",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^14.1.1",
    "sinon": "1.17.3"
  },
  "optionalDependencies": {
    "crypto-browserify": "3.12.0"
